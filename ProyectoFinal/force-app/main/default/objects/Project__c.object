    <validationRules>
        <fullName>Earnings_vs_Losses</fullName>
        <active>false</active>
        <errorConditionFormula>Amount__c  &lt;  EstimatedAmount__c</errorConditionFormula>
        <errorMessage>The project is in loss, please check the estimated amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hours_Not_Covered</fullName>
        <active>false</active>
        <errorConditionFormula>EstimatedHours__c  &lt;  Opportunity__r.TotalOpportunityQuantity</errorConditionFormula>
        <errorMessage>The hours have not been covered, please complete the assignment of resources.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_Editing_Allow_While_In_Progress</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE( Status__c ), &quot;In Progress&quot;)</errorConditionFormula>
        <errorMessage>A project can not be update while is in &apos;In Progress&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Squad_Lead_Assigned</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK( ProjectResource__c  )</errorConditionFormula>
        <errorMessage>Please assign a Squad Lead to the project</errorMessage>
    </validationRules>
